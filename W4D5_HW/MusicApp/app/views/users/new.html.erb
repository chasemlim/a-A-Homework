<!-- sign up page -->
<section class="">
  <div class="">
    <h1>Create an account!</h1>

    <form action="<%= users_url %>" method="post">
      <%= auth_token %>
      <label>
        Email:
        <input type="text" name="user[email]" value="">
      </label>
      <br>
      <label>
        Password:
        <input type="password" name="user[password]" value="">
      </label>
      <br>
      <input type="submit" name="" value="Create account">
    </form>

  </div>
</section>
