<section class="login">
  <div class="login">
    <h1 class="logintext">Welcome back.</h1>

    <form class="forms" action="<%= session_url %>" method="post">
      <%= auth_token %>
      <label class="logintext">
        Email:
        <input type="text" name="user[email]" value="">
      </label>
      <br>
      <label class="logintext">
        Password:
        <input type="password" name="user[password]" value="">
      </label>
      <br>
      <input type="submit" name="" value="Login">
    </form>

  </div>
</section>
